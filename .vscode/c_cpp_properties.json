    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${DefaultIncludePath}","${UserIncludes}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${DefaultIncludePath}"
                ]
            },
            "forcedInclude": [
                "${workspaceFolder}/BUILD/RELEASE/mbed_config.h"
            ],
            "defines": [
                "${DefaultDefines}"
            ],
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "${default}"
        },
        {
            "name": "Linux",
            "includePath": [
                "${DefaultIncludePath}","${UserIncludes}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${DefaultIncludePath}"
                ]
            },
            "defines": [
                "${DefaultDefines}"
            ],
            "forcedInclude": [
                "${workspaceFolder}/BUILD/RELEASE/mbed_config.h"
            ],
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "${default}"
        },
        {
            "name": "Mac",
            "includePath": [
                "${DefaultIncludePath}","${UserIncludes}"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${DefaultIncludePath}"
                ]
